#include <iostream>
void sort_B();
using namespace std;
int main()
{
    cout << "Hello world!" << endl;
    sort_B();
    return 0;
}
void sort_B()
{ int p,n=3,temp,i,j;
int a[3]={3,2,1};
for(p=0;p<n-1;p++)             // Loop for Pass
{

for(j=0;j<n;j++)
{
if(a[j]>a[j+1])
{
temp=a[j];                      // Interchange Values
a[j]=a[j+1];
a[j+1]=temp;
}
}
}
cout<<"\nAfter Sorting : \n";
for(i=0;i<n;i++)
{
cout<<a[i]<<endl;
}
}